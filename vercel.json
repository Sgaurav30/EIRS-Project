{
  "version": 2,
  "buildCommand": "cd client && npm run build",
  "builds": [
    {
      "src": "client/build",
      "use": "@vercel/static"
    },
    {
      "src": "server/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/api/index.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "client/build/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "client/build/index.html"
    }
  ]
}
